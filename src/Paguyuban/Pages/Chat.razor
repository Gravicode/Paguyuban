    protected override async Task OnInitializedAsync()
    {
        NewInbox = new() { Chats = new(), Message = new MessageBox() { }, User = new() };
        NewGroup = new() { Chats = new(), Message = new MessageBox() { }, User = new() };

        timer = new BlazorTimer();
        timer.SetTimer(5000);
        timer.OnElapsed += async () =>
        {
            if (CurrentMessage != null)
            {
                //reload current chat
                await ReloadCurrentChat();
                await InvokeAsync(StateHasChanged);
            }

        };
        var user = (await authenticationStateTask).User;

        if (user.Identity.IsAuthenticated)
        {
            UserName = user.Identity.Name;
            SelUser = UserSvc.GetUserByEmail(UserName);
            if (SelUser == null) SelUser = new();
            IsLogin = true;
            Search();
        }
    }
