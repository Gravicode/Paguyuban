                            <li class="contacts-item  @(GetStyle(item)) @(item.Message.IsRead?"":"unread")">
                                <a class="contacts-link" @onclick:preventDefault @onclick="()=>SelectMessage(item)" href="#">
                                    <div class="avatar @(GetUserStyle(item))">
                                        @if (item.Message.IsGroup)
                                        {
                                            <img class="injectable" src="@(string.IsNullOrEmpty(item.Message.PicGroupUrl)?"./../assets/media/heroicons/outline/user-group.svg":item.Message.PicGroupUrl)" alt="">
                                        }
                                        else
                                        {
                                            <img src="@(string.IsNullOrEmpty(item.User.PicUrl)?"./../assets/media/avatar/2.png":item.User.PicUrl)" alt="">
                                        }

                                    </div>
                                    <div class="contacts-content">
                                        <div class="contacts-info">
                                            <h6 class="chat-name text-truncate">@item.User.FullName</h6>
                                            <div class="chat-time">
                                                @if (!string.IsNullOrEmpty(item.Message.Id))
                                                {
                                                    @DateHelper.GetElapsedTime(item.Message.LastUpdate)

                                                }
                                                else
                                                {
                                                    @("-")
                                                }
                                            </div>
                                        </div>
                                        <div class="contacts-texts">
                                            <p class="text-truncate">
                                                @if (!string.IsNullOrEmpty(item.Message.Id))
                                                {
                                                    @(item.Message.LastMessage?.Length > 50 ? item.Message.LastMessage.Substring(0, 50) : item.Message.LastMessage)
                                                }
                                                else
                                                {
                                                    @("-")
                                                }
                                            </p>
                                            @if (item.Message.UnreadCount > 0)
                                            {
                                                <div class="badge badge-rounded badge-primary ml-1">@item.Message.UnreadCount </div>
                                            }

                                        </div>
                                    </div>
                                </a>
                            </li>
